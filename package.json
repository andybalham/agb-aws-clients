{
  "name": "@andybalham/agb-aws-clients",
  "version": "1.0.4",
  "description": "Collection of AWS client wrappers",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint-and-fix": "eslint ./src --ext .ts --fix",
    "build": "npm run lint && tsc",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test-coverage": "nyc npm run test",
    "deploy-stepfunctions": "tsc && cdk deploy StepFunctionsClientTest",
    "synth-sns": "tsc && cdk synth SNSClientTest",
    "deploy-sns": "tsc && cdk deploy SNSClientTest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybalham/agb-aws-clients.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andybalham/agb-aws-clients/issues"
  },
  "homepage": "https://github.com/andybalham/agb-aws-clients#readme",
  "devDependencies": {
    "@aws-cdk/aws-lambda": "^1.91.0",
    "@aws-cdk/aws-logs": "^1.91.0",
    "@aws-cdk/aws-sns": "^1.91.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.91.0",
    "@aws-cdk/aws-stepfunctions": "^1.91.0",
    "@aws-cdk/aws-stepfunctions-tasks": "^1.91.0",
    "@aws-cdk/core": "^1.91.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "aws-sdk": "^2.853.0",
    "aws-sdk-mock": "^5.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "ts-mock-imports": "^1.3.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@types/sinon": "^9.0.10",
    "sinon": "^9.2.4"
  }
}
